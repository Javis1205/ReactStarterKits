    "server": "cross-conf-env cross-env PORT=npm_package_config_SERVER_PORT NODE_ENV=server NODE_PATH=server FILE_PATH=public/uploads PUBLIC_PATH=../public nodemon --watch server -- server/index.js",
    "server-debug": "scripts/server-debug",
    "dev": "cross-conf-env cross-env PORT=npm_package_config_DEV_PORT SERVER_PORT=npm_package_config_SERVER_PORT NODE_ENV=development nodemon -- webpack-dev-server.js",
    "start": "run-p sass dev",
    "client-rsync": "scripts/client-rsync",
    "server-rsync": "scripts/server-rsync",
    "client-bundle": "scripts/client-bundle",
    "client-deploy": "scripts/client-deploy",
    "public-deploy": "scripts/public-deploy",
    "server-bundle": "scripts/server-bundle",
    "server-deploy": "scripts/server-deploy",
    "bundle": "npm run server-bundle && npm run client-bundle",
